{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://jsonschema.net",
  "properties": {
    "list": {
      "type": "array",
      "id": "http://jsonschema.net/list",
      "items": {
        "type": "object",
        "required": [
          "content",
          "date",
          "tags",
          "url",
          "title"
        ],
        "id": "http://jsonschema.net/list/0",
        "properties": {
          "content": {
            "type": "string",
            "description": "記事の本文です",
            "id": "http://jsonschema.net/list/0/content"
          },
          "date": {
            "type": "string",
            "description": "記事が登録されたDateです",
            "id": "http://jsonschema.net/list/0/date"
          },
          "tags": {
            "type": "array",
            "description": "記事に付けられたタグの配列",
            "id": "http://jsonschema.net/list/0/tags",
            "items": {
              "type": "string",
              "id": "http://jsonschema.net/list/0/tags/0"
            }
          },
          "title": {
            "type": "string",
            "description": "記事のタイトルです",
            "id": "http://jsonschema.net/list/0/title"
          },
          "url": {
            "title": "url",
            "type": "string",
            "description": "記事のURLです",
            "format": "uri",
            "id": "http://jsonschema.net/list/0/url"
          },
          "relatedLinks": {
            "type": "array",
            "items": {
              "type": "object"
            }
          }
        }
      }
    }
  }
}
